    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    
    <!-- script src="js/docs.min.js"></script>
    <script src="js/bootstrap-datepicker.js"></script>
    <script src="js/my.js"></script -->
	<script>


	$(function(){
//date choose

$('#birthday').datepicker({
    format: "mm/dd/yyyy",
    autoclose: true,
}).on('changeDate', function (ev) {
    $(this).datepicker('hide');

});

// cal age of months
var age = [];
var dateCal = function(){
var temp = String($('input[name=birthday]').val()).split('/');

var date = new Date();
if(date.getMonth()>=temp[0]){
age = (date.getFullYear()-temp[2])*12+date.getMonth()+1-temp[0];
}else{
age = (date.getFullYear()-1-temp[2])*12+12+date.getMonth()+1-temp[0];
}
}





// switch unit
	$('input[name=unitRadios]').click(function(){
	
	 if($('input[name=unitRadios]:checked').val() === 'US'){
		$('#unitus').show();
		$('#unitmetric').hide();
	}else{
		$('#unitus').hide();
		$('#unitmetric').show();
	}
	});

//pre send
var weight = [];
var stature = [];
var hc = [];



// load the chart
	$( "#sss" ).submit(function( event ) {
	dateCal();
	if($('input[name=unitRadios]:checked').val() === 'US'){
	
//ready to send 
	weight = parseFloat($('#weightlbs').val()+"."+$('#weightounce').val());
	stature = $('#statureinch').val();
	hc = $('#hcinch').val();
}else{
	weight = $('#weightkg').val()
	stature = $('#staturecm').val();
	hc = $('#hccm').val();
}
	 var formData = {
	    'ktype' : 2,
            'age'    :  age,
            'gender'             : $('input[name=genderRadios]').val(),
	    'weight'             : weight,
	    'stature'             : stature,
	    'hc'             : hc
        };
//send data and load chart
	    $.ajax({
            type        : 'GET', // define the type of HTTP verb we want to use (POST for our form)
            url         : 'http://webservices.onlinegrowthcharts.com/gc', // the url where we want to POST
            data        : formData, // our data object
            dataType    : 'json', // what type of data do we expect back from the server
                        encode          : true
        })
            // using the done promise callback
          	  .done(function(data) {
		       age =formData.age;
		   var bmi = data.bmi;
		  localStorage.setItem("bmi", JSON.stringify(bmi));
		  localStorage.setItem("age", JSON.stringify(age));
                // log data to the console so we can see
		$('#replaced').load('view1.html');
                // here we will handle errors and validation messages
            });
        // stop the form from submitting the normal way and refreshing the page
	event.preventDefault();

});



//
	});
	</script>
  </body>
